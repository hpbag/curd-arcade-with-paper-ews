{
  "name": "curd-arcade",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "concurrently \"redis-server\" \"next dev\"",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.6",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@paperxyz/embedded-wallet-service-sdk": "^0.0.16",
    "@thirdweb-dev/auth": "^3.0.2",
    "@thirdweb-dev/react": "^3.7.2",
    "@thirdweb-dev/sdk": "^3.7.2",
    "cookie": "^0.5.0",
    "date-fns": "^2.29.2",
    "ethers": "^5.6.9",
    "framer-motion": "^6.5.1",
    "next": "^12.2.5",
    "next-seo": "^5.5.0",
    "phaser": "^3.55.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "redis": "^4.2.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.15",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4"
  }
}
